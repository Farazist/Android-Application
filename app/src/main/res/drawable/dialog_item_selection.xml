<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <solid android:color="#54918d"></solid>
            <stroke android:width="@dimen/_1sdp" android:color="#eeeeee"/>
            <corners android:radius="@dimen/_10sdp"></corners>
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <solid android:color="#34716d"></solid>
            <stroke android:width="@dimen/_1sdp" android:color="#eeeeee"/>
            <corners android:radius="@dimen/_10sdp"></corners>
        </shape>
    </item>
</selector>